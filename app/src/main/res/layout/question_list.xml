<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    tools:context=".Controller.Activity.QuestionListActivity">

    <TextView
        android:id="@+id/question_text_list"
        android:layout_width="161dp"
        android:layout_height="47dp"
        android:text="TextView"
        android:textColor="@color/black"
        android:fontFamily="@font/b_yekan"
        />

    <ImageButton
        android:id="@+id/setting_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_toEndOf="@+id/question_text_list"
        android:layout_toRightOf="@+id/question_text_list"
        android:background="#00FFFFFF"
        android:src="@drawable/ic_setting" />

    <CheckBox
        android:id="@+id/answer_list"
        android:layout_width="109dp"
        android:layout_height="45dp"
        android:layout_alignParentRight="true"
        android:text="answer" />


</RelativeLayout>